{
  "name": "textlint-rule-rousseau",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azu/textlint-rule-rousseau.git"
  },
  "author": "azu",
  "email": "azuciao@gmail.com",
  "homepage": "https://github.com/azu/textlint-rule-rousseau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azu/textlint-rule-rousseau/issues"
  },
  "version": "1.4.2",
  "description": "textlint rule check english sentence using rousseau",
  "main": "lib/textlint-rule-rousseau.js",
  "files": [
    "lib",
    "src"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --watch --source-maps",
    "prepublish": "npm run --if-present build",
    "test": "mocha",
    "example": "npm run build && textlint --rulesdir lib README.md"
  },
  "keywords": [
    "textlint",
    "english",
    "proofreader",
    "spellcheck"
  ],
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.2",
    "mocha": "^2.4.5",
    "textlint": "^6.0.1",
    "textlint-tester": "^1.1.0"
  },
  "dependencies": {
    "object-assign": "^4.0.1",
    "rousseau": "^1.0.0",
    "textlint-rule-helper": "^1.2.0",
    "textlint-util-to-string": "^1.2.0"
  }
}
